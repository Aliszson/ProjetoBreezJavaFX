<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>


<AnchorPane prefHeight="600.0" prefWidth="900.0" style="-fx-background-color: #ededed;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.projetomjavafx.AvaliacoesUsuarioController">
   <children>
      <AnchorPane layoutX="150.0" layoutY="100.0" prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #ffffff;">
         <children>
            <TableView fx:id="tableAvaliacoes" focusTraversable="false" layoutX="66.0" layoutY="35.0" prefHeight="314.0" prefWidth="490.0">
              <columns>
                <TableColumn fx:id="colNomeArtista" prefWidth="75.0" text="Música" />
                <TableColumn fx:id="colNomeAlbum" prefWidth="75.0" text="Álbum" />
                  <TableColumn fx:id="colNomeMusica" prefWidth="81.0" text="Artista" />
                  <TableColumn fx:id="colComentario" prefWidth="93.0" text="Comentário" />
                  <TableColumn fx:id="colNota" prefWidth="108.0" text="Nota" />
              </columns>
            </TableView>
         </children>
      </AnchorPane>
      <Text fill="#2a5a5b" layoutX="197.0" layoutY="85.0" strokeType="OUTSIDE" strokeWidth="0.0" text="AVALIAÇÕES">
         <font>
            <Font name="Cambria Bold" size="34.0" />
         </font>
      </Text>
      <ImageView fx:id="voltar" fitHeight="36.0" fitWidth="38.0" layoutX="150.0" layoutY="55.0" onMouseClicked="#onVoltarClick" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../img/icone/setaVoltar.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
